#Function declaration#


FUNCTION multiplyArray(VECTOR[10..0] multiArray, INT multiplier) ->
            VAR count : INT;
            count := 0;

            WHILE _count < 11 DO
                multiArray[_count, 0] := _multiArray[_count, 0] * _multiplier;
            ENDWHILE
        
            RETURN multiArray;        

ENDFUNC

BEGIN
            intVector[0, 0] := 0; 
            intVector[1, 0] := 1; 
            intVector[2, 0] := 2; 
