VAR intVector : VECTOR[10..0];

#Function declaration#

FUNCTION multiplyArray(VECTOR[10..0] multiArray, int multiplier) ->

            VAR count : int;
            count := 0;

            WHILE count < 11 DO:
                multiArray[count, 0] := multiArray[count] * multiplier;
            ENDWHILE
        
            RETURN multiArray;        

ENDFUNC

BEGIN:
            intVector[0, 0] := 0; 
            intVector[1, 0] := 1; 
            intVector[2, 0] := 2; 
            intVector[3, 0] := 3; 
            intVector[4, 0] := 4; 
            intVector[5, 0] := 5; 
            intVector[6, 0] := 6; 
            intVector[7, 0] := 7; 
            intVector[8, 0] := 8; 
            intVector[9, 0] := 9; 
            intVector[10, 0] := 10; 

            intVector := multiplyArray(intVector, 3);

            print(intVector);
END

